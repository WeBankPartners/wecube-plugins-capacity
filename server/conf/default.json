{
  "http": {
    "port": 9096,
    "token": "default-token-used-in-server-side"
  },
  "log": {
    "level": "debug",
    "file": "logs/capacity.log",
    "archive_max_size": 64,
    "archive_max_backup": 10,
    "archive_max_day": 15,
    "compress": true
  },
  "mysql": {
    "server": "127.0.0.1",
    "port": "3306",
    "user": "root",
    "password": "wecube",
    "database": "capacity",
    "maxOpen": 20,
    "maxIdle": 10,
    "timeout": 60
  },
  "cache": {
    "workspace_dir": "public/r_images",
    "clean_interval": 1440
  },
  "data_source": {
    "monitor": {
      "enable": true,
      "base_url": "http://127.0.0.1:8080",
      "token_key": "X-Auth-Token",
      "token_value": "default-token-used-in-server-side"
    }
  }
}